@lost gutter 0px;

$bg-color = red
$text-color = #333

.card
  box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.25);
  cursor pointer
  border-radius 5px
  // lost-utility edit   

button
  lost-utility clearfix
  
  .card-icon
    lost-column 1/3
    text-align right
  
  .card-title
    lost-column 2/3
    text-align left       
    
  	
  
  

	
		
	   
  
  
    
  
  

  